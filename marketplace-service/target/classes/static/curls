curl -X POST -i http://localhost:8082/orders \
     -H "Content-Type: application/json" \
     -d '{
              "user_id": 20,
              "items": [
                { "product_id": 103, "quantity": 1 },
                { "product_id": 104, "quantity": 2 }
              ]
            }'

curl -X POST -i http://localhost:8082/orders \
     -H "Content-Type: application/json" \
     -d '{
              "user_id": 21,
              "items": [
                { "product_id": 105, "quantity": 3 },
                { "product_id": 103, "quantity": 2 }
              ]
            }'

